<script>
	// =================Homepage About us section "ShowMore/ShowLess"=======================//

	function moretext() {
		var x = document.getElementById("rd-more");
		if (x.style.display === "none") {
			x.style.display = "block";
			jQuery("#show-more-less").html('Show Less');
		} else {
			x.style.display = "none";
			jQuery("#show-more-less").html('Show More');
		}
	}
</script>

