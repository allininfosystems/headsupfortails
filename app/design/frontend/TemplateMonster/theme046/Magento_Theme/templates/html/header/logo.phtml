<script>
require(['jquery', 'featuredOwlCarousel'], function () {
jQuery(document).ready(function () {

	//Owl Home page banner 
	var owl = jQuery("#banner-slider-demo-1").owlCarousel({
		 items: 1,
		 autoplay: false,
		 autoplayTimeout: 5000,
		 autoplayHoverPause: true, 
		 dots: true,
		 dotsContainer: '#carousel-custom-dots',
		 nav: true,
		 navRewind: true,
		 // animateIn: 'fadeIn',
		 // animateOut: 'fadeOut',
		 loop: true,
		 navText: ["<em class='porto-icon-chevron-left'></em>","<em class='porto-icon-chevron-right'></em>"],
		 responsiveClass: true,
		 responsive: {
		 991: {
		 dots: true
		 }
		 },
		 startPosition: randomPosition()
		});
		
		 jQuery('.owl-dot').click(function(){
			
			owl.trigger('to.owl.carousel', [jQuery(this).index(), 300]);
			});
		 //Random index generator
		 function randomPosition(){
		 var r_hb = jQuery('#carousel-custom-dots li').length;
		 var randomize = null;
		 
		 
		 randomize=1
		 
		 
		 if (randomize != 0) {
		 return (Math.floor(Math.random()* r_hb));
		 }
		 else { return 0 ;}
		 }

		 //Sort random function
		 function random(owlSelector){
		 owlSelector.children().sort(function(){
		 return Math.round(Math.random()) - 0.5;
		 }).each(function(){
		 jQuery(this).appendTo(owlSelector);
		 });
		 }
	});
});


</script>

<script>
//=================== About-us, FAQs page tab===============//
require(['jquery'], function () {
	jQuery(document).ready(function(){
		
		jQuery('ul.tabs li').click(function(){
			var tab_id = jQuery(this).attr('data-tab');

			jQuery('ul.tabs li').removeClass('current');
			jQuery('.tab-content').removeClass('current');
			jQuery('.tab-content-banner').removeClass('current');

			jQuery(this).addClass('current');
			jQuery("#"+tab_id).addClass('current');
			jQuery("#"+tab_id+"-banner").addClass('current');
		});
		
		var tabName = window.location.hash;
		if(tabName != ""){
			var hash = tabName.split('#')[1];
			jQuery('.'+hash).click();
		}

	});
});
</script>

